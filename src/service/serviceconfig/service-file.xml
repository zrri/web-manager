<!--继承的名字是文件名,继承的文件必须为abstract为true的文件-->
<package name="trade" namespace="file">
	<!-- 文件上传测试 -->
	<service name="upload" class="service.demo.UploadService"
			method="uploadFile">
	</service>
	
	<!-- 文件下载测试 -->
	<service name="download" class="service.demo.DownloadService"
			method="downloadFile">
	</service>
	
	<!-- 大文件上传测试 -->
	<service name="uploadBig" class="service.demo.UploadService"
			method="uploadBigFile">
	</service>
</package>